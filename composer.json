{
  "name": "probiller-ng/purchase-gateway",
  "description": "ProBiller NG Purchase Gateway",
  "keywords": [
    "DDD",
    "Lumen",
    "Doctrine",
    "ProBillerNG"
  ],
  "license": "MIT",
  "repositories": [
    {
      "type": "composer",
      "url": "https://mtl1-satis.mgcorp.co/"
    },
    {
      "type": "composer",
      "url": "https://repository.mgcorp.co/artifactory/api/composer/cose-php-libraries"
    },
    {
      "type": "composer",
      "url": "https://repository.mgcorp.co/artifactory/api/composer/probiller-php-libraries"
    }
  ],
  "type": "project",
  "require": {
    "php": ">=7.1.3 <7.4",
    "ext-apcu": "*",
    "ext-curl": "^7.3",
    "ext-dom": "*",
    "ext-grpc": "^1.35",
    "ext-json": "*",
    "ext-redis": "*",
    "ext-sodium": "*",
    "barryvdh/laravel-cors": "^0.11.3",
    "common-services/email-service-client": "^1.0",
    "common-services/fraud-service-client": "^3.1",
    "doctrine/common": "^2.10",
    "google/cloud-pubsub": "^1.30",
    "google/protobuf": "^3.14",
    "grpc/grpc": "^1.35",
    "guzzlehttp/guzzle": "~6.0",
    "jms/serializer": "^2.2",
    "laravel-doctrine/migrations": "^1.2",
    "laravel-validation-rules/country-codes": "^2.0",
    "laravel/lumen-framework": "^6.0",
    "lcobucci/jwt": "^3.4",
    "mindgeek/aad": "^1.0",
    "myclabs/php-enum": "^1.6",
    "probiller-mgpg/purchase-php-sdk": "2.9.8",
    "probiller-ng/bi-logger": "^2.2",
    "probiller-ng/biller-mapping-service-client": "^2.0",
    "probiller-ng/bin-routing-service-client": "^1.1",
    "probiller-ng/bundle-management-admin-service-client": "^1.0",
    "probiller-ng/cascade-service-client": "^0.9.0",
    "probiller-ng/circuit-breaker": "^1.0",
    "probiller-ng/crypt": "^1.0",
    "probiller-ng/event-ingestion-library": "^1.0",
    "probiller-ng/fraud-service-client": "^1.0",
    "probiller-ng/logger": "^2.4",
    "probiller-ng/lumen-projection-library": "^1.3",
    "probiller-ng/member-profile-gateway-client": "^1.1",
    "probiller-ng/nudata-service": "^1.0",
    "probiller-ng/payment-template-service-client": "^1.3",
    "probiller-ng/purchase-advice-notification-service-client": "^1.0",
    "probiller-ng/service-bus": "^1.2",
    "probiller-ng/site-admin-service-client": "^1.2",
    "probiller-ng/transaction-service-client": "1.21.0",
    "probiller/service-config": "^7.2",
    "ramsey/uuid": "^3.8",
    "ramsey/uuid-doctrine": "^1.5",
    "spatie/valuestore": "^1.2",
    "vlucas/phpdotenv": "^3.3",
    "webpatser/laravel-uuid": "^3.0"
  },
  "require-dev": {
    "appzcoder/lumen-routes-list": "^1.0",
    "fzaninotto/faker": "~1.4",
    "mikey179/vfsstream": "^1.6",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~8.0",
    "probiller-ng/secret-manager": "^2.1",
    "slevomat/coding-standard": "^4.7",
    "squizlabs/php_codesniffer": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "lumen/app/",
      "ProBillerNG\\": "src/",
      "src\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  },
  "archive": {
    "exclude": [
      "/tests/",
      "/public/docs/"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
